     xt$valid_from x4,
     xt$system_to x5,
     _row_id x6,
     xt$id x7,
     xt$system_from x8}
    [:scan
     {:table users,
      :for-valid-time
      [:between #time/date "2021-07-01" #time/date "9999-12-31"]}
     [{id (= id ?_0)}
      xt$valid_to
      _iid
      xt$valid_from
      xt$system_to
      _row_id
      xt$id
      xt$system_from]]]]]]
