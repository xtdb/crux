[:update
 {:table "users"}
 [:rename
  {x3 _iid,
   x5 xt$system_to,
   x6 _row_id,
   x7 xt$id,
   x8 xt$system_from,
   x10 first_name,
   x11 xt$valid_from,
   x12 xt$valid_to}
  [:project
   [x3
    x5
    x6
    x7
    x8
    {x10 "Sue"}
    {x11 (cast-tstz (greatest x4 #time/date "2021-07-01"))}
    {x12 (cast-tstz (least x2 #time/date "9999-12-31"))}]
