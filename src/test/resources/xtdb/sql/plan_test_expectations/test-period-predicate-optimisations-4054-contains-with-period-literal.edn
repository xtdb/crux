[:project
 [{name foo.1/name}]
 [:rename
  foo.1
  [:project
   [name
    _valid_from
    _valid_to
    {_valid_time (period _valid_from _valid_to)}]
   [:scan
    {:table public/foo}
    [name
     {_valid_from
      (<= _valid_from #xt/zoned-date-time "2000-01-01T00:00Z")}
     {_valid_to
      (>=
       (coalesce _valid_to xtdb/end-of-time)
       (coalesce
        #xt/zoned-date-time "2001-01-01T00:00Z"
        xtdb/end-of-time))}]]]]]
